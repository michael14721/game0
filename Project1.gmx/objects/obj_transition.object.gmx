        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Change alpha

if (fade_end == true) {
    exit;
}

if (fade_dir == 0) {
    // Fade to dark
    if (alpha == -1) {
        alpha = 0;
    } else {
        alpha += spd;
    }
    
    if (alpha &gt;= 1) {
        fade_end = true;
        alpha = 1;
    }
} else {
    if (fade_dir == 1) {
        // Fade to light
        if (alpha == -1) {
            alpha = 1;
        } else {
            alpha -= spd;
        }
        
        if (alpha &lt;= 0) {
            fade_end = true;
            alpha = 0;
        }
    } else {
        // Go to 0
        if (alpha != 0) {
            alpha -= sign(alpha) * min(abs(alpha), spd);
        }
    }
}
</string>
