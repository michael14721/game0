            <string>/// Change images of walls
image_speed = 0;

var obj, a, r, u, l, d;
obj = object_index;
a = 1;
r = place_meeting(x + a, y, obj) || x == room_width - 32;
u = place_meeting(x, y - a, obj) || y == 0;
